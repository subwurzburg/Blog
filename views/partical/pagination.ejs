<nav class="my-5" aria-label="Page navigation example">
    <ul class="pagination justify-content-center">
    <li class="page-item 
    <%if(!pagination.hasPre){%>
      disabled
    <%}%>">
        <a class="page-link" href="/?page=<%-pagination.CurrentPage-1%>" tabindex="-1">Previous</a>
      </li>
      <% for (var i = 1 ; i<pagination.PageTotal +1 ; i++){ %>
      <li class="page-item <%if(pagination.CurrentPage==i){%>
          active
        <%}%>
        ">
        <a class="page-link" href="/?page=<%-i%>">
          <%- i %>
        </a>
      </li>
      <% } %>
      <li class="page-item 
      <%if(!pagination.hasNext){%>
        disabled
      <%}%>">
        <a class="page-link" href="/?page=<%-pagination.CurrentPage+1%>">Next</a>
      </li>
    </ul>
  </nav>